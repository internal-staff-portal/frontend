{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn format && react-scripts build",
    "clean": "rimraf dist",
    "format": "prettier -w src && prettier -w public",
    "reinstall": "rimraf node_modules yarn.lock && yarn install",
    "start": "BROWSER=none react-scripts start"
  },
  "precommit": "format",
  "dependencies": {
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
